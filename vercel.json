{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "browse.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "index.html"
    },
    {
      "src": "^/browse(\\.html)?$",
      "dest": "browse.html"
    },
    {
      "src": "^/api/(.*)",
      "dest": "main.py"
    },
    {
      "src": "^/health$",
      "dest": "main.py"
    },
    {
      "src": "^/docs$",
      "dest": "main.py"
    },
    {
      "src": "^/redoc$",
      "dest": "main.py"
    }
  ]
}
