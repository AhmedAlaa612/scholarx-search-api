# Qdrant
QDRANT_API_KEY="your-qdrant-api-key"
QDRANT_ENDPOINT="https://your-cluster.cloud.qdrant.io"
QDRANT_COLLECTION="opportunities"

# PostgreSQL (Supabase)
DB_HOST="your-db-host.supabase.com"
DB_PORT="6543"
DB_NAME="postgres"
DB_USER="your-db-user"
DB_PASSWORD="your-db-password"

# Jina Embeddings
JINA_API_KEY="your-jina-api-key"
JINA_ENDPOINT="https://api.jina.ai/v1/embeddings"
JINA_MODEL="jina-embeddings-v3"

# LLM Providers (query understanding)
GROK_API="your-groq-api-key"
CEREBRAS_API="your-cerebras-api-key"

# App
APP_ENV="development"
LOG_LEVEL="INFO"
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]
